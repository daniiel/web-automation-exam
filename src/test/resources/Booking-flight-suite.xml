<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="book-flight-module">
    <test name="booking flight">
        <parameter name="browser" value="CHROME"/>

        <parameter name="flightType" value="Roundtrip"/>
        <parameter name="flyingFrom" value="LAS"/>
        <parameter name="flyingTo" value="LAX"/>
        <parameter name="numberOfAdults" value="1"/>

        <parameter name="sortByDuration" value="duration:asc"/>
        <classes>
            <class name="com.automation.exam.tests.BookingFlight"/>
        </classes>
    </test>
</suite>